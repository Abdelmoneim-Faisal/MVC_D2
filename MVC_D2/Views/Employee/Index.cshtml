@{
    int i = 1;
}
@model List<Employee>
<div class="container">
    <h2>Sample Table</h2>
    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>SSN</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Birth Date</th>
                <th>Address</th>
                <th>Sex</th>
                <th>Salary</th>
                <th>SUperVisor</th>
                <th>Depatment</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var emp in Model)
            {
                
            <tr>
                <td>@(i++)</td>
                <td>@emp.Fname</td>
                <td>@emp.Lname</td>
                <td>@emp.Bdate</td>
                <td>@emp.Address</td>
                <td>@emp.Sex</td>
                <td>@emp.Salary</td>
                <td>@emp.Superssn</td>
                <td>@emp.Dno</td>
            </tr>
            }
     
        </tbody>
    </table>
</div>
@section Emp{
    ALL EMPLOYEES
}